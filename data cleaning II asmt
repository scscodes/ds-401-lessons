n) | overall average score rating |\n","|AVG_MATH_8_SCORE | fill (mean) | overall average score rating |\n","|AVG_READING_4_SCORE | fill (mean) | overall average score rating |\n","|AVG_READING_8_SCORE | fill (mean) | overall average score rating |\n","\n","\n"],"metadata":{"id":"LmrU85E4RL6u"}},{"cell_type":"code","source":["# define dictionary of key + strategy\n","STRATEGIES = {\n","    'ENROLL': 'median',\n","    'TOTAL_REVENUE': 'median',\n","    'FEDERAL_REVENUE': 'median',\n","    'STATE_REVENUE': 'median',\n","    'LOCAL_REVENUE': 'median',\n","    'TOTAL_EXPENDITURE': 'median',\n","    'INSTRUCTION_EXPENDITURE': 'median',\n","    'SUPPORT_SERVICES_EXPENDITURE': 'median',\n","    'OTHER_EXPENDITURE': 'median',\n","    'CAPITAL_OUTLAY_EXPENDITURE': 'median',\n","    'GRADES_PK_G': 'median',\n","    'GRADES_KG_G': 'median',\n","    'GRADES_4_G': 'median',\n","    'GRADES_8_G': 'median',\n","    'GRADES_12_G': 'median',\n","    'GRADES_1_8_G': 'median',\n","    'GRADES_9_12_G': 'median',\n","    'GRADES_ALL_G': 'median',\n","    'AVG_MATH_4_SCORE': 'mean',\n","    'AVG_MATH_8_SCORE': 'mean',\n","    'AVG_READING_4_SCORE': 'mean',\n","    'AVG_READING_8_SCORE': 'mean'\n","}"],"metadata":{"id":"aQxI8JHxeQyY"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# saving our original df, for easier testing\n","strat_df = df.copy()"],"metadata":{"id":"jgZURO2eiQsb"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["for column, strategy in STRATEGIES.items():\n","    if strategy == 'mean':\n","        strat_df[column] = strat_df[column].fillna(strat_df[column].mean())\n","    elif strategy == 'median':\n","        strat_df[column] = strat_df[column].fillna(strat_df[column].median())"],"metadata":{"id":"hjLTcH1ehqCA"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["missing_mean = strat_df.isnull().mean()*100\n","missing_median = strat_df.isnull().median()*100\n","strat_df_results = pd.DataFrame({'Keys': df.dtypes, 